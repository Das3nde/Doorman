<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid
				android:color="#ee5a09"/>
			<stroke
				android:width="1dp"
				android:color="#f26609"/>
			<corners
				android:radius="10dp" />
			<padding
				android:left="10dp"
				android:top="10dp"
				android:right="10dp"
				android:bottom="10dp" />
		</shape>
	</item>
	<item>
		<shape>
			<gradient
				android:type="linear"
				android:startColor="#ec9052"
				android:endColor="#f26609"
				android:angle="270" />
			<stroke
				android:width="1dp"
				android:color="#f26609" />
			<corners
				android:radius="10dp" />
			<padding
				android:left="10dp"
				android:top="10dp"
				android:right="10dp"
				android:bottom="10dp" />
		</shape>
	</item>
</selector>